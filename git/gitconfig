[user]
	name = juliogreff
	email = juliogreff@gmail.com
[core]
	excludesfile = ~/.gitignore
[color]
	ui = auto
[push]
	default = upstream
[alias]
	lg = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	ci = commit
	ca = commit -a
	ps = push
	psf = push -f
	amend = commit -a --amend -C HEAD
	jenkins = push -f origin HEAD:ci
[diff]
	algorithm = patience
